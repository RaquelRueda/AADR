@model InnovacionDocentes.Models.Encuadre

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="right_col" role="main">

    <div class="">
        <div class="clearfix"></div>
        <div class="row">
            <div class="col-md-12 col-sm-12 ">
                <div class="x_panel">
                    <div class="x_title">
                        <h2>Encuadres <small>detalles del elemento</small></h2>
                        <ul class="nav navbar-right panel_toolbox">
                            <li>
                                <a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
                            </li>

                            <li>
                                <a class="close-link"><i class="fa fa-close"></i></a>
                            </li>
                        </ul>
                        <div class="clearfix"></div>
                    </div>
                    <div class="x_content">
                        <div class="col-md-12 col-sm-12  profile_details">
                            <div class="well profile_view col-md-8">
                                <div class="col-sm-12">

                                    <div class="left col-md-7 col-sm-7">
                                        <h2 class="brief">Encuadres</h2>

                                    </div>
                                    <div class="right col-md-12 col-sm-12 text-center">
                                        <img src="~/Content/production/images/aprendizajesignificativo1_small.png" alt="" class="img-circle img-fluid">
                                    </div>
                                </div>
                                <div class=" profile-bottom text-center">
                                    <p><strong>Fecha inicio: </strong>  </p>
                                    <ul class="list-unstyled">
                                        <li><i class="fa fa-check"></i>   @Html.DisplayFor(model => model.CatTrimestre.strInicio) </li>

                                    </ul>
                                    <p><strong>Fecha fin: </strong>  </p>
                                    <ul class="list-unstyled">
                                        <li><i class="fa fa-check"></i>   @Html.DisplayFor(model => model.CatTrimestre.strFin) </li>

                                    </ul>
                                    <p><strong>Trimestre: </strong>  </p>
                                    <ul class="list-unstyled">
                                        <li><i class="fa fa-check"></i>   @Html.DisplayFor(model => model.CatTrimestre.strDescripcion) </li>

                                    </ul>
                                    <p><strong>Duración: </strong>  </p>
                                    <ul class="list-unstyled">
                                        <li><i class="fa fa-check"></i>   @Html.DisplayFor(model => model.strDuracion) </li>

                                    </ul>
                                    <p><strong>Asignatura: </strong>  </p>
                                    <ul class="list-unstyled">
                                        <li><i class="fa fa-check"></i>   @Html.DisplayFor(model => model.CatAsignatura.strDescripcion) </li>

                                    </ul>
                                    <p><strong>Elaboro: </strong>  </p>
                                    <ul class="list-unstyled">
                                        <li><i class="fa fa-check"></i>   @Html.DisplayFor(model => model.Empleado.strNombre) </li>

                                    </ul>
                                    <p><strong>Tema: </strong>  </p>
                                    <ul class="list-unstyled">
                                        <li><i class="fa fa-check"></i>   @Html.DisplayFor(model => model.Tema.strNombre) </li>

                                    </ul>
                                    <p><strong>Subtema: </strong>  </p>
                                    <ul class="list-unstyled">
                                        <li><i class="fa fa-check"></i>   @Html.DisplayFor(model => model.Contenido.strDescripcion) </li>

                                    </ul>
                                    <p><strong>Proposito: </strong>  </p>
                                    <ul class="list-unstyled">
                                        <li><i class="fa fa-check"></i>   @Html.DisplayFor(model => model.Proposito.strDescripcion) </li>

                                    </ul>
                                    <p><strong>Aprendizaje: </strong>  </p>
                                    <ul class="list-unstyled">
                                        <li><i class="fa fa-check"></i>   @Html.DisplayFor(model => model.idAprendizaje) </li>

                                    </ul>
                                    <p><strong>Inicio: </strong>  </p>
                                    <ul class="list-unstyled">
                                        <li><i class="fa fa-check"></i>   @Html.DisplayFor(model => model.strInicio) </li>

                                    </ul>
                                    <p><strong>Desarrollo: </strong>  </p>
                                    <ul class="list-unstyled">
                                        <li><i class="fa fa-check"></i>   @Html.DisplayFor(model => model.strDesarollo) </li>

                                    </ul>
                                    <p><strong>Cierre: </strong>  </p>
                                    <ul class="list-unstyled">
                                        <li><i class="fa fa-check"></i>   @Html.DisplayFor(model => model.strFin) </li>


                                    </ul>
                                    <p><strong>Cierre: </strong>  </p>
                                    <ul class="list-unstyled">
                                        <li><i class="fa fa-check"></i>   @Html.DisplayFor(model => model.strObservacion) </li>


                                    </ul>
                                    <br />  <br />
                                    <div class=" col-sm-12 emphasis">
                                        <button type="button" class="btn btn-success btn-sm">
                                            <i class="fa fa-back"></i> @Html.ActionLink("Regresar", "Index")
                                        </button>
                                        <button type="button" class="btn btn-primary btn-sm">
                                            <i class="fa fa-pencil"> </i>  @Html.ActionLink("Editar", "Edit", new { id = Model.idEncuadre })
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </div>

                    </div>



                </div>
            </div>
        </div>
    </div>
</div>

